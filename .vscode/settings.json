{
    "editor.rulers": [
        80
    ],
    "files.associations": {
        "*.h": "c",
        "*.c": "c",
        "cstdlib": "c",
        "*.md": "markdown",
        "*.sh": "shellscript",
        "*.cpp": "cpp",
        "*.hpp": "cpp",
        "*.txt": "plaintext",
        "*.json": "jsonc",
        "*.cmake": "cmake",
        "CMakeLists.txt": "cmake",
        "*.py": "python",
        "*.yaml": "yaml",
        "*.yml": "yaml",
        "*.xml": "xml",
        "*.toml": "toml",
        "cstdint": "c"
    },
    "editor.formatOnSave": false,
}